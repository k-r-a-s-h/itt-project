<% include ../partials/header%>
<div class="container">
    <div class="row">
        <h1 style="text-align:center;">Enter the Code</h1>
        <div style="width:30%;margin:25px auto;">
            <form method="POST" action="/admin/login/code">
                <div class="form-group"><input class="form-control" type="hidden" name="username" value="<%=data.email%>" required></div>
                <div class="form-group"><input class="form-control"type="hidden" name="password" placeholder="<%=data.password%>" required></div>
                <div class="form-group"><input class="form-control"type="number" name="code" placeholder="code" required></div>
                <div class="form-group"><input class="btn btn-lg btn-primary btn-block" type="submit" value="Login!" required></div>

                <!-- <input type="submit" value="Login!"> -->
            </form>
        </div>
    </div>
</div>

<% include ../partials/footer%>