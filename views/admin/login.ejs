<% include ../partials/header%>
<div class="container">
    <div class="row">
        <h1 style="text-align:center;">Admin Login</h1>
        <div style="width:30%;margin:25px auto;">
            <form method="POST" action="/admin/login">
                <div class="form-group"><input class="form-control" type="email" name="username" placeholder="email" required></div>
                <div class="form-group"><input class="form-control" type="password" name="password" placeholder="password" required></div>
                <div class="form-group"><input class="btn btn-lg btn-primary btn-block" type="submit" value="Login!" required></div>

                <!-- <input type="submit" value="Login!"> -->
            </form>
        </div>
    </div>
</div>
<% include ../partials/footer%>